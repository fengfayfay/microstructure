    "import brdf;\n",
    "import vec3;\n",
    "import math;\n",
    "import random;\n",
    "y = brdf.ZipinBrdf(.3, .3);\n",
    "wo = vec3.Vec3(random.uniform(0,1), random.uniform(0, 1), random.uniform(0, 1));\n",
    "print(wo)\n",
    "x = y.Sample(wo, (random.uniform(0,1), random.uniform(0, 1)));\n",
    "e = y.Eval(wo, x[2]);\n",
    "print(x[2])\n",
    "print(e, x);"
   ]
